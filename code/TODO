no_low_dict = {}
for key in hyperparameter_dict.keys():
    value = hyperparameter_dict[key]
    mcc_arra = []
    for val in value:
        if val > 0.03:
            mcc_arra.append(val)
    no_low_dict[key] = mcc_arra

